<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.priyanshu.springbootquickstart</groupId>
  <artifactId>course-api</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>Course Api</name>
  
  <!--Step 1 :  Adding spring-boot-starter-parent project as Parent so that we get default configurations-->
  <parent>
  	<groupId>org.springframework.boot</groupId>
	<artifactId>spring-boot-starter-parent</artifactId>
	<version>2.6.3</version>
  </parent>
  
  <!-- Step 2 : Add dependencies-->
  <dependencies>
    <!--This dependency pull all web related jars-->
  	<dependency>
    	<groupId>org.springframework.boot</groupId>
    	<artifactId>spring-boot-starter-web</artifactId>
  	</dependency>
  </dependencies>
  
  <!--Add Properties-->
  <properties>
  	<java.version>17</java.version>
  </properties>
  
</project>


